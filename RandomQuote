
import java.util.ArrayList;

public class RandomQuote {

    public RandomQuote(ArrayList<Quote> q) {
        System.out.println(q.get((int)(Math.random() * q.size())));
        System.out.println(q.get((int)(Math.random() * q.size())));
        System.out.println(q.get((int)(Math.random() * q.size())));
        System.out.println(q.get((int)(Math.random() * q.size())));
        System.out.println(q.get((int)(Math.random() * q.size())));

    }
}
